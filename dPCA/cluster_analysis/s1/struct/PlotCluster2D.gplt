#!/bin/env gnuplot



xsize=3.0
ysize=3.0

set terminal postscript eps enhanced color dl 2.0 size xsize,ysize "Helvetica" 12
set output "tmp.eps"

set size square
set xlabel "PC1"
set ylabel "PC2"
set title  TITLE
set xrange [-3.8:3.2]
set yrange [-3.8:2.2]
set palette defined (0 '#000000',\
1 '#000090',\
2 '#000fff',\
3 '#0090ff',\
4 '#0fffee',\
5 '#90ff70',\
6 '#ffee00',\
7 '#ff7000',\
8 '#ee0000',\
9 '#7f0000')

unset colorbox
plot INPUT u 2:3:4 with points pt 7 ps 0.1 palette notitle


